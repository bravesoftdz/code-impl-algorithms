unit MaiorPreco;

interface

uses
  CarrinhoDeCompras;

type
  TMaiorPreco = class

  public
    function Encontra( const ACarrinhoDeCompras: TCarrinhoDeCompras ): Currency;
  end;

implementation

{ TMaiorPreco }

function TMaiorPreco.Encontra(
  const ACarrinhoDeCompras: TCarrinhoDeCompras): Currency;
begin
  if ACarrinhoDeCompras.getItens.Count = 0 then
    Result:= 0
  else
  begin


    Result:= ACarrinhoDeCompras.getItens[0].getValorTotal()
  end;
end;

end.
